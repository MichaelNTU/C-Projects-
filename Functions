double Addition(int x,int y,int a)
{
	int z;
	 z =x*y*a;
	return z;
}

int main()
{
	int i;
	i =  Addition(4.4, 3.3, 4.6);
	cout<<"The value of I is:"<<i<<endl;
	return 0;
}
