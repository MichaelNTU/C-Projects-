// Element Seperator on Arrays.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
using namespace std;

const int MAX=5;


int _tmain(int argc, _TCHAR* argv[])
{
	double Values[MAX] = {23,56,87,70,90};

	for(int i=0; i<MAX; i++)
	{
		if(i>0)
		{
			cout<<"|";
		}
		cout<<"   "<<Values[i]<<" ";
	}

	return 0;
}
