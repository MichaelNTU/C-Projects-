// Remove element from Array.cpp : Defines the entry point for the console application.
//

#include "stdafx.h"
#include <iostream>
using namespace std;

const int MAX=5;



int _tmain(int argc, _TCHAR* argv[])
{
	double Values[MAX] = {123,156,187,110,90};
	int pos =0;

	for(int i=pos +1; i<MAX;i++)
	{
		Values[i -1]=Values[i];
		Values[i]--;
		cout<<Values[i]+1<<endl;
	}
	//Values[MAX]--;
	//cout<<Values[MAX]<<endl;
	return 0;
}
